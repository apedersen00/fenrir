@006 000FC0  // In_ch0 pos(2,0) weights: [0, -1, 0, 0]
@007 FC0FC0  // In_ch0 pos(2,1) weights: [0, -1, 0, -1]
@008 000FC0  // In_ch0 pos(2,2) weights: [0, -1, 0, 0]
@009 FC0FFF  // In_ch1 pos(0,0) weights: [-1, -1, 0, -1]
@00A FC0F80  // In_ch1 pos(0,1) weights: [0, -2, 0, -1]
@00B FC0FC1  // In_ch1 pos(0,2) weights: [1, -1, 0, -1]
